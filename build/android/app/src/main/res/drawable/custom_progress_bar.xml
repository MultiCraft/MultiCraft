<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape>
			<stroke
				android:width="2.5dp"
				android:color="#000000" />
			<padding
				android:bottom="3dp"
				android:left="3dp"
				android:right="3dp"
				android:top="3dp" />
			<corners android:radius="40dip" />
			<gradient
				android:endColor="#6a6a6a"
				android:startColor="#6a6a6a" />
		</shape>
	</item>

	<item>
		<clip>
			<shape>
				<corners android:radius="40dip" />
			</shape>
		</clip>
	</item>
</layer-list>
